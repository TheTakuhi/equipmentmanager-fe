    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^5.0.1",
    "@tanstack/react-router": "^0.0.1-beta.145",
    "@tanstack/react-table": "^8.10.1",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.6.3",
    "axios": "^1.5.0",
